<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
<PREFIX>
  <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
  <LABEL>owl</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>https://schema.org/</NAMESPACE>
  <LABEL>sdo</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
  <LABEL>rdf</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
  <LABEL>dbpo</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
  <LABEL>dbp</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
  <LABEL>rdfs</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
  <LABEL>dbpedia2</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://purl.org/ontology/mo/</NAMESPACE>
  <LABEL>mo</LABEL>
</PREFIX>
<SOURCE>
  <ID>sourceId</ID>
  <ENDPOINT>C:\Users\johan\Desktop\output.ttl</ENDPOINT>
  <VAR>?s</VAR>
  <PAGESIZE>1000</PAGESIZE>
  <RESTRICTION>?s rdf:type mo:Genre</RESTRICTION>
  <PROPERTY>rdfs:label</PROPERTY>
  <TYPE>N-TRIPLE</TYPE>
</SOURCE>

<TARGET>
  <ID>targetId</ID>
  <ENDPOINT>https://dbpedia.org/sparql</ENDPOINT>
  <VAR>?t</VAR>
  <PAGESIZE>1000</PAGESIZE>
  <RESTRICTION>?t rdf:type dbpo:TopicalConcept</RESTRICTION>
  <PROPERTY>dbpedia2:name</PROPERTY>
  <TYPE>sparql</TYPE>
</TARGET>

<METRIC>
  levenshtein(s.rdfs:label,t.dbpedia2:name)
</METRIC>

<ACCEPTANCE>
  <THRESHOLD>0.4</THRESHOLD>
  <FILE>accepted.nt</FILE>
  <RELATION>owl:sameAs</RELATION>
</ACCEPTANCE>
<REVIEW>
  <THRESHOLD>0.3</THRESHOLD>
  <FILE>reviewme.nt</FILE>
  <RELATION>owl:sameAs</RELATION>
</REVIEW>
<EXECUTION>
  <REWRITER>DEFAULT</REWRITER>
  <PLANNER>DEFAULT</PLANNER>
  <ENGINE>DEFAULT</ENGINE>
</EXECUTION>
<OUTPUT>TTL</OUTPUT>
</LIMES>